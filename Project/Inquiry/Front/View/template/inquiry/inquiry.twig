{% set InquiryModel = 'Project\\Inquiry\\Model\\InquiryModel' %}
{% set TopController = 'Project\\Base\\Front\\Controller\\Front\\TopController' %}
{% set form = self.form %}
<main class="main">
    {{ form.start }}
    <h2 class="lower-title">{{'Contact Us'|tf}}</h2>

    {{ self.containers.Breadcrumb.content | saferaw }}

    <div class="container">
        <div class="step-section">
            <div class="step-wrapper">

                {{ self.getContainer('Steps').getContent() | saferaw }}

                <p class="mt-20">{{'Please select the inquiry category, enter necessary information, and click the "Confirm" button.'|tf}}</p>

                <div class="form-col1">
                    <div class="form-inner {{ form.fieldsets.inquiry.elements.name.errorClass }}">
                        <h4>{{ InquiryModel|ProxyStatic('getPropertyLabel', 'frontName')}}</h4>
                        {{ form.fieldsets.inquiry.elements.name.error | saferaw }}
                        {{ form.fieldsets.inquiry.elements.name | saferaw }}
                    </div>
                    <div class="form-inner {{ form.fieldsets.inquiry.elements.rentalNo.errorClass }}">
                        <h4 class="mt-20">{{ InquiryModel|ProxyStatic('getPropertyLabel', 'frontRentalNo')}}</h4>
                        {{ form.fieldsets.inquiry.elements.rentalNo.error | saferaw }}
                        {{ form.fieldsets.inquiry.elements.rentalNo | saferaw }}
                    </div>
                    <div class="form-inner {{ form.fieldsets.inquiry.elements.email.errorClass }}">
                        <h4 class="mt-20">{{ InquiryModel|ProxyStatic('getPropertyLabel', 'frontEmail')}}</h4>
                        {{ form.fieldsets.inquiry.elements.email.error | saferaw }}
                        {{ form.fieldsets.inquiry.elements.email | saferaw }}
                    </div>
                    <div class="form-inner {{ form.fieldsets.inquiry.elements.phone.errorClass }}">
                        <h4 class="mt-20">{{ InquiryModel|ProxyStatic('getPropertyLabel', 'frontPhone')}}</h4>
                        {{ form.fieldsets.inquiry.elements.phone.error | saferaw }}
                        {{ form.fieldsets.inquiry.elements.phone | saferaw }}
                    </div>
                </div>
                <div class="form-col2">

                    <div class="form-inner {{ form.fieldsets.inquiry.elements.InquiryType.errorClass }}">
                        <h4 class="mt-20">{{ InquiryModel|ProxyStatic('getPropertyLabel', 'frontInquiryType')}}</h4>
                        {{ form.fieldsets.inquiry.elements.InquiryType.error | saferaw }}
                        <div class="select-box">
                            {{ form.fieldsets.inquiry.elements.InquiryType | saferaw }}
                            <div class="select-box-icon">
                                <span class="ion-ios-arrow-down icon"></span>
                            </div>
                        </div>
                    </div>

                    <div class="form-inner {{ form.fieldsets.inquiry.elements.InquiryAction.errorClass }}">
                        <h4 class="mt-20">{{ InquiryModel|ProxyStatic('getPropertyLabel', 'frontInquiryAction')}}</h4>
                        {{ form.fieldsets.inquiry.elements.InquiryAction.error | saferaw }}
                        <div class="select-box">
                            {{ form.fieldsets.inquiry.elements.InquiryAction | saferaw }}
                            <div class="select-box-icon">
                                <span class="ion-ios-arrow-down icon"></span>
                            </div>
                        </div>
                    </div>

                </div>

                <div class="form-col1">
                    <div class="form-inner {{ form.fieldsets.inquiry.elements.body.errorClass }}">
                        <h4 class="mt-20">{{ InquiryModel|ProxyStatic('getPropertyLabel', 'frontBody')}}</h4>
                        {{ form.fieldsets.inquiry.elements.body.error | saferaw }}
                        {{ form.fieldsets.inquiry.elements.body | saferaw }}
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="cv-area">
        {{ form.elements.submit | saferaw }}
    </div>

    {% do form.end %}
</main>